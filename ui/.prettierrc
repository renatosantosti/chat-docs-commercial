{
  "semi": true,
  "singleQuote": false,
  "trailingComma": "all",
  "printWidth": 80,
  "tabWidth": 2,
  "arrowParens": "always",
  "endOfLine": "lf",
  "plugins": ["prettier-plugin-packagejson"],
  "importOrder": ["^react$", "<THIRD_PARTY_MODULES>", "", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
